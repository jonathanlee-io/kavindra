{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kavindra-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "js-widget/**/*"
    ],
    "webpack": true,
    "tsConfigPath": "apps/kavindra-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/kavindra-api",
  "projects": {

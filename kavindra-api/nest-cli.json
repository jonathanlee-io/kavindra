      "type": "application",
      "root": "apps/kavindra-api",
      "entryFile": "main",
      "sourceRoot": "apps/kavindra-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kavindra-api/tsconfig.app.json"
      }
    },
